{
  "name": "svelte-mobx-example",
  "description": "MobX connector for Svelte example",
  "version": "1.0.0",
  "repository": "https://github.com/xelaok/svelte-mobx-example",
  "author": "Alexander Kozlovich <alexander.kozlovich@gmail.com>",
  "keywords": [
    "svelte",
    "mobx",
    "mvvm"
  ],
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public --single",
    "start:dev": "sirv public --single --dev",
    "format": "prettier --write src"
  },
  "dependencies": {
    "sirv-cli": "^1.0.10"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^2.3.4",
    "mobx": "^6.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.0",
    "rollup": "^2.35.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-root-import": "^1.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "svelte": "^3.31.0",
    "svelte-mobx": "^1.0.0",
    "svelte-preprocess": "^4.6.1",
    "typescript": "^4.1.3"
  }
}
